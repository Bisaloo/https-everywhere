<!--
	For other ticketmaster rules, see Ticketmaster.xml

	Time out:
		ticketmaster.eu

	Invalid certificate:
		www.ticketmaster.eu
		www.ch.ticketmaster.eu

	Login required:
		cloudtest.ticketmaster.eu

	No working URL known:
		downloads.platinum.ticketmaster.eu
		media.platinum.ticketmaster.eu
		www.zz.ticketmaster.eu

	Refused:
		zz.ticketmaster.eu

-->
<ruleset name="Ticketmaster.eu">

	<target host="api.ticketmaster.eu" />
	<target host="app.ticketmaster.eu" />
	<target host="media.ticketmaster.eu" />
		<test url="http://media.ticketmaster.eu/germany/help/images/kartenruecksendung/kartenruecksendung-de-2017-03.pdf" />
	<target host="reg.ticketmaster.eu" />

	<rule from="^http:" to="https:" />

</ruleset>
