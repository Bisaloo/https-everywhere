<!--
	For more ticketmaster rules, see Ticketmaster.xml

	Refused:
		ticketmaster.es

	Invalid certificate:
		click.email.ticketmaster.es
		image.email.ticketmaster.es
		promotores.ticketmaster.es

	Time out:
		view.email.ticketmaster.es

	Different content HTTP/HTTPS:
		mi.ticketmaster.es

	Mixed content:
		platinum.ticketmaster.es

-->
<ruleset name="Ticketmaster.es">

	<target host="ticketmaster.es" />
	<target host="www.ticketmaster.es" />
	<target host="ajuda.ticketmaster.es" />
	<target host="ayuda.ticketmaster.es" />
	<target host="blog.ticketmaster.es" />
	<target host="help.ticketmaster.es" />

	<rule from="^http://ticketmaster\.es/"
		to="https://www.ticketmaster.es/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
