<ruleset name="Epson.com">

	<target host="epson.com" />
	<target host="www.epson.com" />
	<target host="latin.epson.com" />
	<target host="global.latin.epson.com" />

	<target host="epson.ca" />
	<target host="www.epson.ca" />
	
	<target host="epson.cl" />
	<target host="www.epson.cl" />

	<target host="epson.co.cr " />
	<target host="www.epson.co.cr " />

	<target host="epson.com.ar" />
	<target host="www.epson.com.ar" />

	<target host="epson.com.bo" />
	<target host="www.epson.com.bo" />

	<target host="epson.com.br" />
	<target host="www.epson.com.br" />

	<target host="epson.com.co" />
	<target host="www.epson.com.co" />

	<target host="epson.com.do" />
	<target host="www.epson.com.do" />

	<target host="epson.com.ec" />
	<target host="www.epson.com.ec" />

	<target host="epson.com.jm" />
	<target host="www.epson.com.jm" />

	<target host="epson.com.mx" />
	<target host="www.epson.com.mx" />

	<target host="epson.com.pe" />
	<target host="www.epson.com.pe" />

	<target host="epson.com.py" />
	<target host="www.epson.com.py" />

	<target host="epson.com.uy" />
	<target host="www.epson.com.uy" />

	<target host="epson.com.ve" />
	<target host="www.epson.com.ve" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://www\.epson\.(cl|co\.cr|com\.(ar|bo|br|co|do|ec|jm|mx|pe|py|uy|ve))/"
		to="https://epson.$1/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
