	Invalid certificate:
		pos.epson.com
		www.epson.com.mx

	No working URL known:
		was.epson.com
		cform.epson.jp

	Redirect to http:
		shop.epson.jp

	Time out:
		global.epson.com
		epson.jp
		www.espon.jp

-->
<ruleset name="Epson.com">

	<target host="epson.com" />
	<target host="www.epson.com" />
	<target host="latin.epson.com" />
	<target host="global.latin.epson.com" />

	<target host="epson.ca" />
	<target host="www.epson.ca" />

	<target host="epson.com.mx" />
	<target host="www.epson.com.mx" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://www\.epson\.com\.mx/"
		to="https://epson.com.mx/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
