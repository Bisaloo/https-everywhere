<!--
	For other ticketmaster rules, see Ticketmaster.xml

	Refused:
		ticketmaster.de

	Invalid certificate:
		coldplay.ticketmaster.de
		depeche-mode.ticketmaster.de
		click.email.ticketmaster.de
		image.email.ticketmaster.de
		kundenbewertungen.ticketmaster.de
		losgehts.ticketmaster.de
		reviews.ticketmaster.de

	Time out:
		view.email.ticketmaster.de
		ntr.ticketmaster.de

	No working URL known:
		telekom.ticketmaster.de

-->
<ruleset name="Ticketmaster.de">

	<target host="ticketmaster.de" />
	<target host="www.ticketmaster.de" />
	<target host="blog.ticketmaster.de" />
	<target host="help.ticketmaster.de" />
	<target host="hilfe.ticketmaster.de" />

	<rule from="^http://ticketmaster\.de/"
		to="https://www.ticketmaster.de/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
