<!--
	For other Ticketmaster rules, see Ticketmaster.xml

	Refused:
		ticketweb.co.uk

	Invalid certificate:
		click.e.ticketweb.co.uk
		image.e.ticketweb.co.uk
		click.email.ticketweb.co.uk
		image.email.ticketweb.co.uk
		listings.ticketweb.co.uk

	Time out:
		view.email.ticketweb.co.uk
-->
<ruleset name="Ticketweb.co.uk">

	<target host="ticketweb.co.uk" />
	<target host="www.ticketweb.co.uk" />
	<target host="help.ticketweb.co.uk" />

	<rule from="^http://ticketweb\.co\.uk/"
		to="https://www.ticketweb.co.uk/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
