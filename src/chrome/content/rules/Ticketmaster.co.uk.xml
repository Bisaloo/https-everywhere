<!--
	For other ticketmaster rules, see Ticketmaster.xml

	Invalid certificate:
		ticketmaster.co.uk
		getstarted.ticketmaster.co.uk
		listings.ticketmaster.co.uk
		click.mailings.ticketmaster.co.uk
		image.mailings.ticketmaster.co.uk
		origin.media.ticketmaster.co.uk
		reviews.ticketmaster.co.uk
		summit.ticketmaster.co.uk
		talk.ticketmaster.co.uk

	Time out:
		view.mailings.ticketmaster.co.uk
		ntr.ticketmaster.co.uk

	Mixed content:
		platinum.ticketmaster.co.uk
		platinumni.ticketmaster.co.uk

	Connection reset:
		survey.ticketmaster.co.uk

	Different content HTTP/HTTPS:
		theatre.ticketmaster.co.uk

-->
<ruleset name="Ticketmaster.co.uk">

	<target host="ticketmaster.co.uk" />
	<target host="www.ticketmaster.co.uk" />
	<target host="attractions.ticketmaster.co.uk" />
	<target host="blog.ticketmaster.co.uk" />
	<target host="guides.ticketmaster.co.uk" />
	<target host="help.ticketmaster.co.uk" />
	<target host="m.ticketmaster.co.uk" />
	<target host="media.ticketmaster.co.uk" />
	<target host="www1.ticketmaster.co.uk" />

	<rule from="^http:" to="https:" />

</ruleset>
