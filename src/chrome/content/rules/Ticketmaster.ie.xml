<!--
	Invalid certificate:
		ticketmaster.ie
		click.email.ticketmaster.ie
		image.email.ticketmaster.ie
		getstarted.ticketmaster.ie
		reviews.ticketmaster.ie

	Time out:
		view.email.ticketmaster.ie
		ntr.ticketmaster.ie

	Mixed content:
		platinum.ticketmaster.ie

-->
<ruleset name="Ticketmaster.ie">

	<target host="ticketmaster.ie" />
	<target host="www.ticketmaster.ie" />
	<target host="blog.ticketmaster.ie" />
	<target host="help.ticketmaster.ie" />
	<target host="m.ticketmaster.ie" />

	<rule from="^http://ticketmaster\.ie/"
		to="https://www.ticketmaster.ie/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
