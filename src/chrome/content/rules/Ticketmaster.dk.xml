<!--
	For other Ticketmaster rules, see Ticketmaster.xml

	Refused:
		ticketmaster.dk

	Invalid certificate:
		b2b.ticketmaster.dk
		kundeanmeldelser.ticketmaster.dk
		click.mailings.ticketmaster.dk
		wmr.ticketmaster.com

	Time out:
		billetsalg.ticketmaster.dk
		image.mailings.ticketmaster.dk
		view.mailings.ticketmaster.dk

-->
<ruleset name="Ticketmaster.dk">

	<target host="ticketmaster.dk" />
	<target host="www.ticketmaster.dk" />
	<target host="help.ticketmaster.dk" />
	<target host="kundeservice.ticketmaster.dk" />

	<rule from="^http://ticketmaster\.dk/"
		to="https://www.ticketmaster.dl/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
