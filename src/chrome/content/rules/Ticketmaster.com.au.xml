<!--
	For other ticketmaster rules, see Ticketmaster.xml

	Redirect to HTTP:
		www.ticketmaster.com.au

	Invalid certificate:
		getstarted.ticketmaster.com.au
		click.mailings.ticketmaster.com.au
		image.mailings.ticketmaster.com.au
		click.mailing.resale.ticketmaster.com.au
		reviews.ticketmaster.com.au
		summit.ticketmaster.com.au

	Time out:
		view.mailings.ticketmaster.com.au
		ntr.ticketmaster.com.au

	Mixed content:
		platinum.ticketmaster.com.au

-->
<ruleset name="Ticketmaster.com.au">

	<target host="ticketmaster.com.au" />
	<target host="blog.ticketmaster.com.au" />
	<target host="help.ticketmaster.com.au" />
	<target host="kaspa.ticketmaster.com.au" />
	<target host="m.ticketmaster.com.au" />
	<target host="resale.ticketmaster.com.au" />
	<target host="ticketing.ticketmaster.com.au" />

	<rule from="^http:" to="https:" />

</ruleset>
