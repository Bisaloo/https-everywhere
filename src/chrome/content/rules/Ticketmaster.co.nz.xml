<!--
	For more ticketmaster rules, see Ticketmaster.xml

	Redirect to HTTP:
		www.ticketmaster.co.nz

	Invalid certificate:
		getstarted.ticketmaster.co.nz
		click.mailings.ticketmaster.co.nz
		image.mailings.ticketmaster.co.nz
		reviews.ticketmaster.co.nz

	Time out:
		view.mailings.ticketmaster.co.nz
		ntr.ticketmaster.co.nz

	Mixed content:
		platinum.ticketmaster.co.nz

-->
<ruleset name="Ticketmaster.co.nz">

	<target host="ticketmaster.co.nz" />
	<target host="blog.ticketmaster.co.nz" />
	<target host="help.ticketmaster.co.nz" />
	<target host="m.ticketmaster.co.nz" />
	<target host="resale.ticketmaster.co.nz" />
	<target host="www1.ticketmaster.co.nz" />

	<rule from="^http:" to="https:" />

</ruleset>
