<!--
	For more ticketmaster rules, see Ticketmaster.xml

	Redirect to HTTP:
		www.ticketmaster.com.mx

	Invalid certificate:
		eguia.ticketmaster.com.mx
		reviews.ticketmaster.com.mx
		tmntr.ticketmaster.com.mx
		www2.ticketmaster.com.mx

	Time out:
		ntr.ticketmaster.com.mx

-->
<ruleset name="Ticketmaster.com.mx">

	<target host="ticketmaster.com.mx" />
	<target host="m.ticketmaster.com.mx" />
	<target host="tmespeciales.ticketmaster.com.mx" />
	<target host="www1.ticketmaster.com.mx" />

	<rule from="^http:" to="https:" />

</ruleset>
